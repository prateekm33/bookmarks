{
  "manifest_version": 2,

  "name": "Bookmarks",
  "description": "This helps you keep your bookmarks in check",
  "version": "1.3",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Access your bookmarks!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "bookmarks",
    "tabs"
  ],

  "omnibox": {"keyword" : "bmarks"},
  "icons": {"16" : "icon.png"}
}